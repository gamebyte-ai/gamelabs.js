{
  "name": "gamelabsjs",
  "version": "0.1.0",
  "description": "Gamelabsjs: a game framework powered by Three.js (3D) and PixiJS (2D).",
  "license": "MIT",
  "author": "Oguz Sandikci",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oguzsan/Gamelab.js.git"
  },
  "bugs": {
    "url": "https://github.com/oguzsan/Gamelab.js/issues"
  },
  "homepage": "https://github.com/oguzsan/Gamelab.js#readme",
  "keywords": [
    "game",
    "framework",
    "threejs",
    "three",
    "pixijs",
    "pixi",
    "2d",
    "3d"
  ],
  "peerDependencies": {
    "@pixi/layout": "^3.2.0",
    "@pixi/ui": "^2.3.2",
    "@types/three": "^0.160.0 || ^0.182.0",
    "pixi.js": "^8.0.0",
    "three": "^0.160.0 || ^0.182.0"
  },
  "peerDependenciesMeta": {
    "@types/three": {
      "optional": true
    }
  },
  "devDependencies": {
    "@pixi/layout": "^3.2.0",
    "@pixi/ui": "^2.3.2",
    "@types/three": "^0.182.0",
    "pixi.js": "^8.0.0",
    "rimraf": "^6.0.0",
    "three": "^0.182.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run clean && tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  }
}
